public class Obstacle4 {
	public Sprite image;
	public Rectangle bounds;
	public int counter;
	
	public Obstacle4(){
		image = Assets.obstacle2;
		bounds = new Rectangle(1920,0,450,128);
		counter = 0;
	}

}
